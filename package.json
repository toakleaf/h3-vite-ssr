{
  "name": "h3-vite-ssr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx server/index.ts",
    "dev:log": "ENABLE_LOGGING=true tsx server/index.ts",
    "build:client": "vite build",
    "build:ssr": "vite build --ssr src/entry-server.tsx --outDir .output/server",
    "build:runtime": "vite build --mode server-runtime",
    "build": "tsc -b && pnpm run build:client && pnpm run build:ssr && pnpm run build:runtime",
    "start": "NODE_ENV=production node .output/server/index.js",
    "start:log": "NODE_ENV=production ENABLE_LOGGING=true node .output/server/index.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/node": "^24.0.8",
    "compression": "^1.8.0",
    "h3": "^1.15.3",
    "listhen": "^1.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "serve-static": "^2.2.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/compression": "^1.8.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/serve-static": "^1.15.8",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5"
  }
}
